<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:state_pressed="true">
        <shape>
            <solid android:color="@color/colorPrimary2" />
            <stroke android:width="@dimen/w_1" android:color="@color/colorPrimary2" />
            <corners android:radius="@dimen/round_90" />
        </shape>
    </item>
    <item>
        <shape>
            <gradient android:angle="270" android:endColor="@color/White" android:startColor="@color/White" />
            <stroke android:width="@dimen/w_1" android:color="@color/White" />
            <corners android:radius="@dimen/round_90" />
        </shape>
    </item>

</selector>